{
  "name": "five-bells-integration-test",
  "version": "1.4.0",
  "description": "A module to help with testing Five Bells components against each other",
  "main": "src/index.js",
  "bin": {
    "integration": "src/bin/integration"
  },
  "scripts": {
    "test": "node ./src/bin/integration all",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/interledger/five-bells-integration-test.git"
  },
  "keywords": [
    "ilp",
    "interledger",
    "test",
    "integration",
    "qa"
  ],
  "author": "Stefan Thomas <justmoon@members.fsf.org>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/interledger/five-bells-integration-test/issues"
  },
  "homepage": "https://github.com/interledger/five-bells-integration-test#readme",
  "devDependencies": {
    "cz-conventional-changelog": "^1.1.5",
    "eslint": "^2.0.0-rc.1",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.0.8",
    "eslint-plugin-standard": "^1.3.2",
    "ghooks": "^1.0.3",
    "validate-commit-msg": "^2.0.0"
  },
  "config": {
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "bluebird": "^3.3.0",
    "bluebird-co": "^2.1.2",
    "co-mocha": "^1.1.2",
    "five-bells-condition": "^3.0.0",
    "mocha": "^2.4.5",
    "node-fetch": "^1.3.3",
    "superagent": "^1.7.2",
    "wait-on": "^1.3.1"
  }
}
